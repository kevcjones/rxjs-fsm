{
  "name": "rxjs-fsm",
  "version": "1.0.7",
  "description": "An rxjs finite statemachine, i needed one for an Angular project but the code i made is just rxjs and typescript",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "test": "mocha -r ts-node/register lib/**/*.spec.ts",
    "coverage": "nyc mocha"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "exclude": [
      "dist",
      "coverage",
      "test",
      "**/*.spec.ts",
      "lib/index.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevCJones/rxjs-fsm.git"
  },
  "keywords": [
    "typescript",
    "fsm",
    "rxjs",
    "finite",
    "state",
    "machine"
  ],
  "author": "Kevin Jones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KevCJones/rxjs-fsm/issues"
  },
  "homepage": "https://github.com/KevCJones/rxjs-fsm#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "jsdom": "^15.1.1",
    "mocha": "^10.2.0",
    "nyc": "^14.1.0",
    "rxjs": "^6.5.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "typescript": "~3.4.5"
  },
  "dependencies": {}
}
